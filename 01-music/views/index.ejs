<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <header>
    	<h1><%= title %></h1>
    	<ul id="type" class="type">
    	    <li data-type="dot">点状</li>
    	    <li data-type="column" class="selected">柱状</li>
    	</ul>
    	<p>
    		音量<input type="range" id="vol" min="0" max="100" value="60">
    	</p>
    	
    </header>
    <div class="left">
    	<ul id="list">
    		<% music.forEach(function(name){ %>
    			<li title="<%=name%>"><%=name%></li>
    		<% }) %>
    	</ul>
    </div>
    <div class="right" id="box"></div>
    <script type="text/javascript" src="/javascripts/index.js"></script>
  </body>
</html>
